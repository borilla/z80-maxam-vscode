{
    "name": "maxam",
    "displayName": "Maxam Z80",
    "description": "Syntax highlighting for Maxam Z80 assembly language",
    "version": "1.0.0",
    "author": "borilla",
    "publisher": "borilla",
    "repository": {
        "type": "git",
        "url": "https://github.com/borilla/z80-maxam-vscode.git"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "maxam",
            "aliases": ["maxam", "z80"],
            "extensions": [".asm", ".z80"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "maxam",
            "scopeName": "source.maxam",
            "path": "./syntaxes/maxam.tmLanguage"
        }],
        "themes": [{
            "label": "Maxam Z80",
            "uiTheme": "vs-dark",
            "path": "./syntaxes/maxam.tmTheme"
        }]
    }
}